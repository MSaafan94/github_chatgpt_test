<odoo>
    <data>
        <template id="portal_ment" name="Maintenance Requisition">
            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True"/>
                <t t-call="portal.portal_searchbar">
                    <t t-set="title">Maintenance</t>
                </t>
                <div class="row mt-2 mb-2">
                    <div class="col-12">
                        <a href="/my/maintenance_request_insert_view" class="create_new btn bg-gradient-info">
                            <i class="fas fa-shopping-basket"></i>New Maintenance Request
                        </a>
                    </div>
                </div>
                <div t-if="not ments" class="alert alert-info">There are currently no Maintenance Request for your
                    account.
                </div>
                <t t-if="ments" t-call="portal.portal_table">
                    <thead>
                        <tr class = "text-center">
                            <th>#</th>
                            <th>Number</th>
                            <th>Maintenance</th>
                            <th>Maintenance Type</th>
                            <th>Request Date</th>
                            <th>Schedule Date</th>
                            
                            <th class="text-center">Stage</th>
                            <th class="text-center">Action</th>
                        </tr>
                    </thead>
                    <t t-set="i" t-value="1"/>
                    <t t-foreach="ments" t-as="ment">
                        <tr class = "text-center">
                            <td class="text-center">
                                <span t-esc="i"/>
                                <t t-set="i" t-value="i+1"/>
                            </td>
                            <td class="text-center">
                                <span t-field="ment.name"/>
                            </td>
                            <td  class="text-center">
                                <span t-field="ment.maintenance_type_custom"/>
                            </td>
                            <td class="text-center">
                                <t t-if = "ment.maintenance_type_custom == 'order'">
                                    <span t-field="ment.order_type_id"/>
                                </t>
                                <t t-else = "">
                                    <span t-field="ment.repair_type_id"/>
                                </t>
                            </td>
                            <td class="text-center">
                                <span t-field="ment.request_date"></span>
                            </td>
                            <td class="text-center">
                                <span t-field="ment.schedule_date"></span>
                            </td>
                            <td class="text-center">
                                <span t-field="ment.state"/>
                            </td>
                            <td class="text-center py-0 align-middle">
                                <div class="btn-group btn-group-sm">
                                    <a t-attf-href="/my/maintenance_request/#{ment.id}" class="btn btn-info">
                                        <i class="fas fa-eye"></i>View
                                    </a>
                                    <a t-if="ment.state == 'draft' and not approval"
                                       t-attf-href="/my/maintenance_request/confirm/#{ment.id}"
                                       class="btn btn-success">
                                        <i class="fas fa-check"></i>Confirm
                                    </a>
                                    <a t-if="ment.state == 'draft' and not approval"
                                       t-attf-href="/my/maintenance_request/edit/#{ment.id}"
                                       class="btn btn-success">
                                        <i class="fas fa-edit"></i>Edit
                                    </a>
                                    <a t-if="ment.state == 'draft' and not approval"
                                       t-attf-href="/my/maintenance_request/delete/#{ment.id}"
                                       class="btn btn-danger">
                                        <i class="fas fa-trash"></i>Delete
                                    </a>
                                    <a t-if="approval and ment.state == 'submited'"
                                       t-attf-href="/my/maintenance_request/submit/#{ment.id}"
                                       class="btn btn-success">
                                        <i class="fas fa-check"></i>Submit
                                    </a>
                                    <a t-if="approval and ment.state == 'dm_to_approve'"
                                       t-attf-href="/my/maintenance_request/dm/#{ment.id}"
                                       class="btn btn-success">
                                        <i class="fas fa-check"></i>Direct Manager Approval
                                    </a>
                                    <a t-if="approval and ment.state == 'om_to_approve'"
                                       t-attf-href="/my/maintenance_request/om/#{ment.id}"
                                       class="btn btn-success">
                                        <i class="fas fa-check"></i>Operation Manager Approve
                                    </a>
                                    <a t-if="approval and ment.state == 'mm_to_approve'"
                                       t-attf-href="/my/maintenance_request/mm/#{ment.id}"
                                       class="btn btn-success">
                                        <i class="fas fa-check"></i>Maintenance Manager Approve
                                    </a>
                                </div>
                            </td>
                        </tr>
                    </t>
                </t>
            </t>
        </template>
        <template id="maintenance_request_submit" name="Maintenance: Submit a Request">

            <t t-call="portal.portal_layout">
                <br/>
                <div class="card card-info">
                    <div class="card-header">
                        <h3 class="card-title">New Maintenance Request</h3>
                    </div>
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form action="/maintenance/insert" method="post" id="maintenance_form"
                          enctype="multipart/form-data">
                        <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                        <div class="card-body o_portal_leaves_details">

                            <div class="form-group">
                                <label>Maintenance Type</label>
                                <select class="form-control  maintenance_type_custom" required="required"
                                        name="maintenance_type_custom">
                                    <option value="" selected="selected"
                                            disabled="disabled">Select Maintenance Type
                                    </option>
                                    <option
                                            value="order">
                                        Order
                                    </option>
                                    <option
                                            value="repair">
                                        Repair
                                    </option>
                                </select>
                            </div>


                            <div class="form-group order_type" style="display : none;">
                                <label>Order Type</label>
                                <select class="form-control "
                                        name="order_type_id">
                                    <option value="" selected="selected"
                                            disabled="disabled">Select Order Type
                                    </option>
                                    <t t-foreach="order_types" t-as="order_type">
                                        <option
                                                t-att-value="order_type.id">
                                            <t t-esc="order_type.name"/>
                                        </option>
                                    </t>
                                </select>
                            </div>
                            <!--                            <div class="form-group order_type" style="display : none;">-->
                            <!--                                <label>Order Type</label>-->
                            <!--                                <select class="form-control "-->
                            <!--                                        name="order_type_id">-->
                            <!--                                    <option value="" selected="selected"-->
                            <!--                                            disabled="disabled">Scheduled Date 2-->
                            <!--                                    </option>-->
                            <!--                                    <t t-foreach="order_types" t-as="order_type">-->
                            <!--                                        <option-->
                            <!--                                                t-att-value="order_type.id">-->
                            <!--                                            <t t-esc="order_type.name"/>-->
                            <!--                                        </option>-->
                            <!--                                    </t>-->
                            <!--                                </select>-->
                            <!--                            </div>-->
                            <div class="form-group repair_type" style="display : none;">
                                <label>Repair Type</label>
                                <select class="form-control"
                                        name="repair_type_id">
                                    <option value="" selected="selected"
                                            disabled="disabled">Select Repair Type
                                    </option>
                                    <t t-foreach="repair_types" t-as="repair_type">
                                        <option
                                                t-att-value="repair_type.id">
                                            <t t-esc="repair_type.name"/>
                                        </option>
                                    </t>
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="description">Description</label>
                                <textarea type="text" class="form-control name" name="name" required="required"/>
                            </div>
                            <div class="form-group">
                                <label>Priority</label>
                                <select class="form-control  priority" required="required"
                                        name="priority">
                                    <option value="" selected="selected"
                                            disabled="disabled">
                                    </option>
                                    <option
                                            value="0">
                                        Very Low
                                    </option>
                                    <option
                                            value="1">
                                        Low
                                    </option>
                                    <option
                                            value="2">
                                        Medium
                                    </option>
                                    <option
                                            value="3">
                                        High
                                    </option>
                                </select>

                            </div>
                            <!--                            <div class="form-group">-->
                            <!--                                <label class="control-label" for="schedule_date">Scheduled Date</label>-->
                            <!--                                <input type="date" name="schedule_date" required="required"-->
                            <!--                                       class="form-control date_to schedule_date"-->
                            <!--                                       placeholder="DD/MM/YYYY"/>-->
                            <!--                            </div>-->
                            <!--                            changed-->
                            <div class="form-group">
                                <label>Scheduled Period</label>
                                <select class="form-control" required="required"
                                        name="schedule_date">
                                    <option value="" selected="selected"
                                            disabled="disabled">
                                    </option>
                                    <option
                                            value="school_week">
                                        School week
                                    </option>
                                    <option
                                            value="summer_break">
                                        Summer Break
                                    </option>
                                    <option
                                            value="winter_break">
                                        Winter Break
                                    </option>
                                    <option
                                            value="fall_break">
                                        Fall Break
                                    </option>
                                    <option
                                            value="spring_break">
                                        Spring Break
                                    </option>
                                </select>
                            </div>
                            <!--                            changed-->
                            <div class="form-group">
                                <label class="control-label" for="location">Location</label>
                                <input type="text" class="form-control date_to location" t-att-value="location"
                                       name="location"/>

                            </div>
                            <div class="form-group">
                                <label class="control-label" for="date_to">Employee Name</label>
                                <input type="text" t-att-value="employee_name" readonly="1"/>

                            </div>
                            <div class="form-group">
                                <label class="control-label">Employee Room Number</label>
                                <input type="text" t-att-value="employee_room_number" readonly="1"/>

                            </div>
                            <div class="form-group">
                                <label for="attachment">Maintenance Attachments</label>
                                <div class="input-group">
                                    <input type="file" name="attachment" multiple="multiple"
                                           data-show-upload="true" data-show-caption="true" lass="file"
                                           data-show-preview="true" accept=".png,.jpg,.jpeg,.pdf,image/*"
                                    />
                                </div>
                            </div>
                        </div>
                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                    </form>
                </div>
            </t>
        </template>

